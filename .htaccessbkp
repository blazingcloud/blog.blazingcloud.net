RewriteEngine On
RewriteCond %{HTTP_HOST} ^www\.blazingcloud\.net$ [NC]
RewriteRule ^(.*)$ http://blazingcloud.net/$1 [L,R=301]


# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /wordpress/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /wordpress/index.php [L]
</IfModule>

# END WordPress
